<Ui xmlns="http://www.blizzard.com/wow/ui" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <Frame name="ZKFFormTemplate" movable="true" enableMouse="true" virtual="true">
    <Size>
      <AbsDimension x="60" y="300" />
    </Size>
    <Anchors>
      <Anchor point="CENTER" relativeTo="UIParent">
        <Offset>
          <AbsDimension x="0" y="0" />
        </Offset>
      </Anchor>
    </Anchors>
    <TitleRegion setAllPoints="true" />
    <Backdrop bgFile="Interface\DialogFrame\UI-DialogBox-Background">
      <TileSize>
        <AbsValue val="32" />
      </TileSize>
      <EdgeSize>
        <AbsValue val="32" />
      </EdgeSize>
    </Backdrop>
    <Frames>
	  <Button name="$icon" text="$parentButton2">
        <Size>
          <AbsDimension x="32" y="32" />
        </Size>
        <Anchors>
          <Anchor point="TOPLEFT">
            <Offset>
              <AbsDimension x="9" y="-32" />
            </Offset>
          </Anchor>
        </Anchors>
        <Scripts>
          <OnEnter>myIconEnter()</OnEnter>
          <OnLeave>myIconLeave()</OnLeave>
          <OnClick>myIconClick()</OnClick>
        </Scripts>
        <NormalTexture file="Interface\Icons\Ability_Druid_Berserk"/>
        <PushedTexture file="Interface\Icons\Ability_Druid_Berserk"/>
        <DisabledTexture file="Interface\Icons\Ability_Druid_Berserk"/>
        <HighlightTexture file="Interface\Buttons\UI-Panel-Button-Highlight" alphaMode="ADD"/>
        <ButtonText name="$parentText">
          <FontHeight>
            <AbsValue val="10" />
          </FontHeight>
        </ButtonText>
        <NormalFont style="GameFontNormal" />
        <HighlightFont style="GameFontHighlight" />
        <DisabledFont style="GameFontDisable" />
      </Button>
      <Button name="$parentButton1" text="Bonus">
        <Size>
          <AbsDimension x="60" y="20" />
        </Size>
        <Anchors>
          <Anchor point="TOPLEFT">
            <Offset>
              <AbsDimension x="20" y="-180" />
            </Offset>
          </Anchor>
        </Anchors>
        <NormalTexture file="Interface\Buttons\UI-Panel-Button-Up">
          <TexCoords left="0" right="0.625" top="0" bottom="0.6875" />
        </NormalTexture>
        <PushedTexture file="Interface\Buttons\UI-Panel-Button-Down">
          <TexCoords left="0" right="0.625" top="0" bottom="0.6875" />
        </PushedTexture>
        <DisabledTexture file="Interface\Buttons\UI-Panel-Button-Disabled">
          <TexCoords left="0" right="0.625" top="0" bottom="0.6875" />
        </DisabledTexture>
        <HighlightTexture file="Interface\Buttons\UI-Panel-Button-Highlight" alphaMode="ADD">
          <TexCoords left="0" right="0.625" top="0" bottom="0.6875" />
        </HighlightTexture>
        <ButtonText name="$parentText">
          <FontHeight>
            <AbsValue val="8" />
          </FontHeight>
        </ButtonText>
        <NormalFont style="GameFontNormalSmall" />
        <HighlightFont style="GameFontHighlight" />
        <DisabledFont style="GameFontDisable" />
        <Scripts>
        	<OnClick>myBonusClick()</OnClick>
        </Scripts>
      </Button>
      <Button name="$parentComponent1" text="No Bonus">
        <Size>
          <AbsDimension x="60" y="20" />
        </Size>
        <Anchors>
          <Anchor point="TOPLEFT">
            <Offset>
              <AbsDimension x="20" y="-200" />
            </Offset>
          </Anchor>
        </Anchors>
        <NormalTexture file="Interface\Buttons\UI-Panel-Button-Up">
          <TexCoords left="0" right="0.625" top="0" bottom="0.6875" />
        </NormalTexture>
        <PushedTexture file="Interface\Buttons\UI-Panel-Button-Down">
          <TexCoords left="0" right="0.625" top="0" bottom="0.6875" />
        </PushedTexture>
        <DisabledTexture file="Interface\Buttons\UI-Panel-Button-Disabled">
          <TexCoords left="0" right="0.625" top="0" bottom="0.6875" />
        </DisabledTexture>
        <HighlightTexture file="Interface\Buttons\UI-Panel-Button-Highlight" alphaMode="ADD">
          <TexCoords left="0" right="0.625" top="0" bottom="0.6875" />
        </HighlightTexture>
        <ButtonText name="$parentText">
          <FontHeight>
            <AbsValue val="8" />
          </FontHeight>
        </ButtonText>
        <NormalFont style="GameFontNormalSmall" />
        <HighlightFont style="GameFontHighlight" />
        <DisabledFont style="GameFontDisable" />
        <Scripts>
        	<OnClick>myNoBonusClick()</OnClick>
        </Scripts>
      </Button>
      <Button name="$parentComponent2" text="Off Spec">
        <Size>
          <AbsDimension x="60" y="20" />
        </Size>
        <Anchors>
          <Anchor point="TOPLEFT">
            <Offset>
              <AbsDimension x="20" y="-220" />
            </Offset>
          </Anchor>
        </Anchors>
        <NormalTexture file="Interface\Buttons\UI-Panel-Button-Up">
          <TexCoords left="0" right="0.625" top="0" bottom="0.6875" />
        </NormalTexture>
        <PushedTexture file="Interface\Buttons\UI-Panel-Button-Down">
          <TexCoords left="0" right="0.625" top="0" bottom="0.6875" />
        </PushedTexture>
        <DisabledTexture file="Interface\Buttons\UI-Panel-Button-Disabled">
          <TexCoords left="0" right="0.625" top="0" bottom="0.6875" />
        </DisabledTexture>
        <HighlightTexture file="Interface\Buttons\UI-Panel-Button-Highlight" alphaMode="ADD">
          <TexCoords left="0" right="0.625" top="0" bottom="0.6875" />
        </HighlightTexture>
        <ButtonText name="$parentText">
          <FontHeight>
            <AbsValue val="8" />
          </FontHeight>
        </ButtonText>
        <NormalFont style="GameFontNormalSmall" />
        <HighlightFont style="GameFontHighlight" />
        <DisabledFont style="GameFontDisable" />
        <Scripts>
        	<OnClick>myOffSpecClick()</OnClick>
        </Scripts>
      </Button>
      <Button name="$parentComponent3" text="Pass">
        <Size>
          <AbsDimension x="60" y="20" />
        </Size>
        <Anchors>
          <Anchor point="TOPLEFT">
            <Offset>
              <AbsDimension x="20" y="-240" />
            </Offset>
          </Anchor>
        </Anchors>
        <NormalTexture file="Interface\Buttons\UI-Panel-Button-Up">
          <TexCoords left="0" right="0.625" top="0" bottom="0.6875" />
        </NormalTexture>
        <PushedTexture file="Interface\Buttons\UI-Panel-Button-Down">
          <TexCoords left="0" right="0.625" top="0" bottom="0.6875" />
        </PushedTexture>
        <DisabledTexture file="Interface\Buttons\UI-Panel-Button-Disabled">
          <TexCoords left="0" right="0.625" top="0" bottom="0.6875" />
        </DisabledTexture>
        <HighlightTexture file="Interface\Buttons\UI-Panel-Button-Highlight" alphaMode="ADD">
          <TexCoords left="0" right="0.625" top="0" bottom="0.6875" />
        </HighlightTexture>
        <ButtonText name="$parentText">
          <FontHeight>
            <AbsValue val="8" />
          </FontHeight>
        </ButtonText>
        <NormalFont style="GameFontNormalSmall" />
        <HighlightFont style="GameFontHighlight" />
        <DisabledFont style="GameFontDisable" />
        <Scripts>
        	<OnClick>myPassClick()</OnClick>
        </Scripts>
      </Button>
    </Frames>
  </Frame>
</Ui>